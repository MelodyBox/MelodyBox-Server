{
  "name": "melodybox-server",
  "version": "0.1.0",
  "private": "true",
  "description": "The server code for the MelodyBox-Client",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "node dist/index.js",
    "serve": "npm run build && npm run start",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MelodyBox/MelodyBox-Server.git"
  },
  "author": "MrPancakes39",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/MelodyBox/MelodyBox-Server/issues"
  },
  "homepage": "https://github.com/MelodyBox/MelodyBox-Server#readme",
  "dependencies": {
    "express": "^4.18.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.7",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
